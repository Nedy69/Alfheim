<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:models="clr-namespace:Alfheim.ViewModels"
             xmlns:views="clr-namespace:Alfheim.Views"
             xmlns:controls="clr-namespace:Alfheim.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Alfheim.Views.ForwardKinematic">
    <Border
        Background="Transparent"
        CornerRadius="25"
        Padding="4">
        <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto" Margin="25">
            <views:Articulation Grid.Row="0"></views:Articulation>
            <views:Articulation Grid.Row="1"></views:Articulation>
            <views:Articulation Grid.Row="2"></views:Articulation>
            <views:Articulation Grid.Row="3"></views:Articulation>
            <views:Articulation Grid.Row="4"></views:Articulation>
            <views:Articulation Grid.Row="5"></views:Articulation>
            <Button Grid.Row="6" HorizontalAlignment="Center" Margin="10">Go ALL!!!</Button>
        </Grid>
    </Border>
</UserControl>